<h1>Edit Song Form:</h1>

<%= form_for(@song) do |f| %>
  <%= f.label :title%><br>
  <%= f.text_field :title%><br>
  <%= f.label :released%><br><br>
  <%= f.check_box :released %><br>
  <%= f.label :release_year%><br><br>
  <%= f.number_field :release_year %><br>
  <%= f.label :artist_name%><br><br>
  <%= f.text_field :artist_name %><br>
  <%= f.label :genre%><br><br>
  <%= f.text_field :genre %><br>
  <%= f.submit %>
  <% end %>
